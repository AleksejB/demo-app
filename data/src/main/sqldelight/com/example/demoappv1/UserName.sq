CREATE TABLE user_name_table (
    user_id INTEGER AS Int NOT NULL PRIMARY KEY,
    user_last_name TEXT NOT NULL,
    user_first_name TEXT NOT NULL
);

insert:
INSERT OR IGNORE INTO user_name_table VALUES(:userId, :userLastName, :userFirstName);

updateUserName:
UPDATE user_name_table
SET user_last_name = :userLastName
WHERE user_id = :userId;

getUserName:
SELECT user_first_name, user_last_name FROM user_name_table
WHERE user_id = :userId;


